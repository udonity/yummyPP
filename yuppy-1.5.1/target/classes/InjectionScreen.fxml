<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root fitToHeight="true" fitToWidth="true" prefHeight="557.0" prefWidth="384.0" type="ScrollPane" xmlns="http://javafx.com/javafx/15" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="550.0" minWidth="365.0" prefHeight="552.0" prefWidth="365.0" snapToPixel="false">
         <children>
            <Label text="マイページURLを入力" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets left="10.0" top="30.0" />
               </VBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TextField fx:id="clientURL" />
            <Label text="マイページで読み込むページ数（デフォルト3ページ)" VBox.vgrow="ALWAYS">
               <font>
                  <Font size="14.0" />
               </font>
               <VBox.margin>
                  <Insets left="10.0" top="5.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="myPageSearchRange" />
            <Label text="ランキングページで上下ランクいくつ読み込むか（デフォルト上下5人分)" VBox.vgrow="ALWAYS">
               <font>
                  <Font size="14.0" />
               </font>
               <VBox.margin>
                  <Insets left="10.0" top="5.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="rankSearchRange" />
            <Label text="他者のページで何ページ分読み込むか（デフォルト1ページ）">
               <font>
                  <Font size="14.0" />
               </font>
               <VBox.margin>
                  <Insets left="10.0" top="5.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="pageSearchRange" />
            <Label text="グローバルランキングか日本ランキングか（&quot;jp&quot;または&quot;global&quot;で記入、　デフォルトグローバル）">
               <font>
                  <Font size="14.0" />
               </font>
               <VBox.margin>
                  <Insets left="10.0" top="5.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="country" />
            <Label text="★の最小値（未記入は絞り込まない）">
               <font>
                  <Font size="14.0" />
               </font>
               <VBox.margin>
                  <Insets left="10.0" top="5.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="starMin" />
            <Label text="★の最大値（未記入は絞り込まない）">
               <font>
                  <Font size="14.0" />
               </font>
               <VBox.margin>
                  <Insets left="10.0" top="5.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="starMax" />
            <Label text="PPの最小値（未記入は絞り込まない）">
               <font>
                  <Font size="14.0" />
               </font>
               <VBox.margin>
                  <Insets left="10.0" top="5.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="ppMin" prefHeight="25.0" prefWidth="408.0" />
